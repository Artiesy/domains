{{ header }}{{ column_left }}
<div id="content" class="cookiepolicy-module js-editor">
  <div class="container-fluid">
    <div class="content-head">
        <h1>{{ text_title }}</h1>
        <ul class="breadcrumb">
            {% for breadcrumb in breadcrumbs %}
                <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
            {% endfor %}
        </ul>

        <div class="pull-right">
            <button type="submit" form="form-cookiepolicy" class="btn btn-primary js-save">{{ button_apply }}</button>
            <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default">{{ button_close }}</a>
        </div>
    </div> <!-- /.content-head -->

      {% if error_warning %}
      <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
        <button type="button" class="close" data-dismiss="alert">&times;</button>
      </div>
      {% endif %}
      {% if success %}
      <div class="alert alert-success alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ success }}
        <button type="button" class="close" data-dismiss="alert">&times;</button>
      </div>
      {% endif %} <!-- /.messages -->

    <div class="content-body">
        <div class="panel panel-default panel-body cp-main-panel">
        <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-module" class="form-horizontal">

          <div class="tab-navigation" style="position:relative">
            <ul class="nav nav-tabs cookiepolicy-nav-base" id="main-tabs">
                <li class=""><a href="#tab_core_settings" data-toggle="tab" aria-expanded="false">{{ tab_core_settings }}</a></li>
                <li class="active"><a href="#tab_text_settings" data-toggle="tab" aria-expanded="true">{{ tab_text_settings }}</a></li>
                <li class=""><a href="#tab_view_settings" data-toggle="tab" aria-expanded="false">{{ tab_view_settings }}</a></li>
            </ul>
          </div>

        <div class="tab-content">
            <div class="tab-pane fade" id="tab_core_settings">
            <div class="row">
                <div class="form-group">
                    <label class="col-sm-2 form-label">{{ entry_status }}</label>
                        <div class="col-sm-10">
                            <div class="btn-group btn-radio">
                                <input type="radio" name="module_cookiepolicy_status" id="module_cookiepolicy_status_1" value="1" hidden {{ module_cookiepolicy_status ? 'checked' : '' }}>
                                <label for="module_cookiepolicy_status_1" class="btn btn-default btn-active-success">{{ text_enabled }}</label>
                                <input type="radio" name="module_cookiepolicy_status" id="module_cookiepolicy_status_0" value="0" hidden {{ module_cookiepolicy_status ? '' : 'checked' }}>
                                <label for="module_cookiepolicy_status_0" class="btn btn-default btn-active-danger">{{ text_disabled }}</label>
                            </div>
                        </div>
                </div>

                <div class="form-group">
                    <label class="col-sm-2 form-label" for="input-cookie-expires"><span data-toggle="tooltip" title="{{ help_cookie_expires }}">{{ entry_cookie_expires }}</span></label>
                      <div class="col-sm-10">
                        <input type="text" name="module_cookiepolicy_cookie_expires" value="{{ module_cookiepolicy_cookie_expires }}" placeholder="{{ text_cookie_expires }}" id="input-cookie-expires" class="form-control" />
                        <small class="form-text text-muted"> {{ help_cookie_expires }}</small>
                      </div>
                </div>
                <!--<div class="form-group">
                    <label class="col-sm-2 form-label" for="input-cookie-name"><span data-toggle="tooltip" title="{{ help_cookie_name }}">{{ entry_cookie_name }}</span></label>
                      <div class="col-sm-10">
                        <input type="text" name="module_cookiepolicy_cookie_name" value="{{ module_cookiepolicy_cookie_name }}" placeholder="{{ text_cookie_name }}" id="input-cookie-name" class="form-control" />
                        <small class="form-text text-muted"> {{ help_cookie_name }}</small>
                      </div>
                </div>-->
            </div>
            </div><!-- /.tab_core_settings --> 

            <div class="tab-pane fade active in" id="tab_text_settings">
            <div class="form-group">
                <label class="col-sm-2 col-form-label"><span data-toggle="tooltip" title="{{ help_cookietext }}">{{ entry_cookietext }}</span></label>
                <div class="col-sm-10">
				<small class="form-text text-muted"> {{ help_cookietext }}</small>
                    {% for language in languages %}
                    <div class="input-group"><span class="input-group-addon">
                        <img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}"  alt=""/></span>
                    <input name="module_cookiepolicy_cookietext[text][{{ language.code }}]" value="{{ attribute(module_cookiepolicy_cookietext.text, language.code) }}" placeholder="{{ text_cookietext }}" id="cookietext_{{ language.code }}" class="form-control" />
                    </div>
                   {% endfor %}
                </div>
            </div> 

            <div class="form-group">
                <div class="js-toggle">
                    <div class="radio form-group form-group-radio">
                            <label class="col-sm-2 form-label label-radio">
                            <input type="radio" class="col-form-label" name="module_cookiepolicy_infopage_on" value="1" {{ module_cookiepolicy_infopage_on ? 'checked' : 'checked' }}>
                            {{ entry_infopage }}
                            </label>
                            <div class="js-toggle-target col-sm-10" {{ not module_cookiepolicy_infopage_on ? 'style="display:none"' : '' }}>
                                <select name="module_cookiepolicy_infopage" id="input-cookiepolicy-infopage" class="form-control">
                                <option value="0">{{ text_none }}</option>
                                {% for information in informations %}
                                {% if information.information_id == module_cookiepolicy_infopage %}
                                <option value="{{ information.information_id }}" selected="selected">{{ information.title }}</option>
                                {% else %}
                                <option value="{{ information.information_id }}">{{ information.title }}</option>
                                {% endif %}
                                {% endfor %}
                                </select>
                                <small class="form-text text-muted"> {{ help_infopage }}</small><br />
                                <small class="form-text text-muted"> {{ help_or_random }}</small>
                            </div>
                    </div>
                    <div class="radio form-group form-group-radio">
                            <label class="col-sm-2 form-label label-radio">
                            <input type="radio" name="module_cookiepolicy_infopage_on" value="0" {{ not module_cookiepolicy_infopage_on ? 'checked' : '' }}>
                            {{ entry_url }}
                            </label>
                            <div class="js-toggle-target col-sm-10" {{ module_cookiepolicy_infopage_on ? 'style="display:none"' : '' }}>
                                <small class="form-text text-muted"> {{ help_random_url }}</small>
                                <div class="input-group"><span class="input-group-addon">&nbsp<i class="fa fa-link" aria-hidden="true"></i></span>
                                <input name="module_cookiepolicy_url" value="{{ module_cookiepolicy_url }}" placeholder="{{ text_url }}" class="form-control"></input>
                                </div>
                                <small class="form-text text-muted"> {{ help_random_url_text }}</small>
                                {% for language in languages %}
                                <div class="input-group"><span class="input-group-addon">
                                <img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}"  alt=""/></span>
                                <input name="module_cookiepolicy_urltext[text][{{ language.code }}]" value="{{ attribute(module_cookiepolicy_urltext.text, language.code) }}" placeholder="{{ text_urltext }}" id="urltext_{{ language.code }}" class="form-control" />
                                </div>
                                {% endfor %}
                            </div>
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-2 form-label">{{ entry_blank_page }}</label>
                    <div class="col-sm-10">
                        <div class="btn-group btn-radio">
                            <input type="radio" name="module_cookiepolicy_blank_page" id="module_cookiepolicy_blank_page_blank" value="_blank" hidden {% if module_cookiepolicy_blank_page == '_blank' %}checked{% endif %}>
                            <label for="module_cookiepolicy_blank_page_blank" class="btn btn-default btn-active-success">{{ text_yes }}</label>
                            <input type="radio" name="module_cookiepolicy_blank_page" id="module_cookiepolicy_blank_page_self" value="_self" hidden {% if module_cookiepolicy_blank_page == '_self' %}checked{% endif %}>
                            <label for="module_cookiepolicy_blank_page_self" class="btn btn-default btn-active-danger">{{ text_no }}</label>
                        </div>
                    </div>
            </div>

            <div class="form-group">
                <label class="col-sm-2 form-label"><span data-toggle="tooltip" title="{{ help_cookietext_after }}">{{ entry_cookietext_after }}</span></label>
                <div class="col-sm-10">
                <small class="form-text text-muted"> {{ help_cookietext_after }}</small>
                {% for language in languages %}
                    <div class="input-group"><span class="input-group-addon">
                    <img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}"  alt=""/></span>
                    <input name="module_cookiepolicy_cookietext_after[text][{{ language.code }}]" value="{{ attribute(module_cookiepolicy_cookietext_after.text, language.code) }}" placeholder="{{ text_cookietext_after }}" id="cookietext_after_{{ language.code }}" class="form-control" />
                    </div>
                {% endfor %}
                </div>
            </div> 

            <div class="form-group">
                <label class="col-sm-2 form-label"><span data-toggle="tooltip" title="{{ help_buttontext }}">{{ entry_buttontext }}</span></label>
                <div class="col-sm-10">
                <small class="form-text text-muted"> {{ help_buttontext }}</small>
                {% for language in languages %}
                    <div class="input-group"><span class="input-group-addon">
                    <img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}"  alt=""/></span>
                    <label for="buttontext_{{ language.code }}"></label><input name="module_cookiepolicy_buttontext[text][{{ language.code }}]" value="{{ attribute(module_cookiepolicy_buttontext.text, language.code) }}" placeholder="{{ text_buttontext }}" id="buttontext_{{ language.code }}" class="form-control" />
                    </div>
                {% endfor %}
                </div>
            </div> 

            </div>

            <div class="tab-pane fade" id="tab_view_settings">
              <div class="form-group">
                <label class="col-sm-2 form-label" for="cookie-position">{{ entry_position }}</label>
                  <div class="col-sm-10">    
                    <select name="module_cookiepolicy_position" id="cookie-position" class="form-control">
                    {% if module_cookiepolicy_position == "bottom" %} 
                    <option value="bottom" selected="selected">{{ text_bottom }}</option>
                    <option value="top">{{ text_top }}</option>
                    {% elseif module_cookiepolicy_position == "top" %}
                    <option value="bottom">{{ text_bottom }}</option>
                    <option value="top" selected="selected">{{ text_top }}</option>
                    {% else %}
                    <option value="bottom" selected="selected">{{ text_bottom }}</option>
                    <option value="top">{{ text_top }}</option>
                    {% endif %}
                    </select>
                 </div>
             </div>

            <div class="form-group">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">{{ text_text_view }}</span></h3>
                </div>
            <div class="panel-body">
