                        <small class="form-text text-muted"> {{ help_text_colour }}</small>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 form-label" for="input-link-colour"><span data-toggle="tooltip" title="{{ help_link_colour }}">{{ entry_link_colour }}</span></label>
                        <div class="col-sm-2">
                            <input type="text" name="module_cookiepolicy_link_colour" value="{{ module_cookiepolicy_link_colour }}" id="input-link-colour" class="jscolor {required:false,hash:true} form-control"  />
                            <small class="form-text text-muted"> {{ help_link_colour }}</small>
                        </div>
                    <label class="col-sm-2 form-label" for="input-link-hover"><span data-toggle="tooltip" title="{{ help_link_hover }}">{{ entry_link_hover }}</span></label>
                        <div class="col-sm-2">
                            <input type="text" name="module_cookiepolicy_link_hover" value="{{ module_cookiepolicy_link_hover }}" id="input-link-hover" class="jscolor {required:false,hash:true} form-control" />
                            <small class="form-text text-muted"> {{ help_link_hover }}</small>
                        </div>
                </div>

                <div class="form-group row">
